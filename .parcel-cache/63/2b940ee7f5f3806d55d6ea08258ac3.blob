/* ============ FONTS END ============ */
/* ============ CSS RESET START ============ */
/* http://meyerweb.com/eric/tools/css/reset/ 
     v2.0 | 20110126
     License: none (public domain)
  */
html,
body,
div,
span,
applet,
object,
iframe,
h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote,
pre,
a,
abbr,
acronym,
address,
big,
cite,
code,
del,
dfn,
em,
img,
ins,
kbd,
q,
s,
samp,
small,
strike,
strong,
sub,
sup,
tt,
var,
b,
u,
i,
center,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
legend,
table,
caption,
tbody,
tfoot,
thead,
tr,
th,
td,
article,
aside,
canvas,
details,
embed,
figure,
figcaption,
footer,
header,
hgroup,
menu,
nav,
output,
ruby,
section,
summary,
time,
mark,
audio,
video {
  margin: 0;
  padding: 0;
  border: 0;
  font-size: 100%;
  font: inherit;
  vertical-align: baseline;
}
/* HTML5 display-role reset for older browsers */
article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section {
  display: block;
}
body {
  line-height: 1;
}
ol,
ul {
  list-style: none;
}
blockquote,
q {
  quotes: none;
}
blockquote:before,
blockquote:after,
q:before,
q:after {
  content: "";
  content: none;
}
table {
  border-collapse: collapse;
  border-spacing: 0;
}
/* ============ CSS RESET END ============ */
/* ============ COMMON STYLES START ============ */
*,
::before,
::after {
  box-sizing: border-box;
}
::selection {
  background-color: #fbd784;
  color: #000;
}
html {
  scroll-behavior: smooth;
}
:root {
  --brand-color: #fbd784;
  --bg-color: #0b1d26;
}
a,
span,
::before,
::after {
  display: inline-flex;
  text-decoration: none;
  color: inherit;
}
body {
  font-family: "Gilroy", system-ui, -apple-system, "Segoe UI", Roboto,
    "Helvetica Neue", Arial, "Noto Sans", "Liberation Sans", sans-serif,
    "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol", "Noto Color Emoji";
  font-size: 18px;
  font-weight: 700;
  background-color: var(--bg-color);
  color: #fff;
}
a {
  color: #fff;
  padding: 10px;
  transition: 0.3s;
}
a:hover {
  color: var(--brand-color);
}
.wide-container {
  max-width: 1790px;
  width: 100%;
  margin: 0 auto;
  padding: 0 15px;
  position: relative;
}
.container {
  max-width: 1490px;
  width: 100%;
  margin: 0 auto;
  padding: 0 15px;
}
.small-title {
  display: inline-flex;
  align-items: center;
  color: var(--brand-color);
  letter-spacing: 6px;
  text-transform: uppercase;
}
.small-title::before {
  content: "";
  width: 72px;
  height: 2px;
  background-color: var(--brand-color);
  margin-right: 30px;
}
/* ============ COMMON STYLES END ============ */
/* ============ HEADER START ============ */
.header {
  padding-top: 50px;
  position: absolute;
  width: 100%;
  left: 0;
  top: 0;
  z-index: 1;
}
.nav {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.logo {
  font-family: "Chronicle Display", serif;
  font-size: 32px;
  font-weight: 700;
  letter-spacing: 0.01em;
  margin-left: -10px;
}
.menu {
  display: flex;
}
.menu li:nth-child(n + 2) {
  margin-left: 20px;
}
.account {
  margin-right: -10px;
  column-gap: 10px;
}
/* ============ HEADER END ============ */
/* ============ HERO START ============ */
.hero-section {
  background: linear-gradient(
      344.24deg,
      rgba(11, 29, 38, 0) 49.06%,
      var(--bg-color) 108.93%
    ),
    linear-gradient(180deg, rgba(11, 29, 38, 0) 58%, var(--bg-color) 86.38%),
    url("7578e51ba0e649eab9c1b67ee5705eb1") no-repeat 50% / cover;
  padding-bottom: 1110px;
}
.hero-section a {
  padding: 10px;
  transition: 0.3s;
}
.hero-section a:hover {
  color: var(--brand-color);
}
.content {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-top: 290px;
}
.social-block {
  display: flex;
  align-items: center;
  transform: rotateZ(90deg);
  margin-left: -77px;
}
.social-networks {
  display: flex;
  margin-left: 15px;
}
.social-networks a {
  padding: 10px;
  font-size: 24px;
  transform: rotateZ(-90deg);
}
.social-networks li:nth-child(n + 2) {
  margin-left: 10px;
}
.social-networks a:hover {
  color: var(--brand-color);
}
.offer {
  width: 963px;
  margin: 0 auto;
}
.offer .swiper-slide * {
  margin-top: 30px;
}
.offer .swiper-slide *:first-child {
  margin-top: 0;
}
.offer h3 {
  font-family: "Chronicle Display", serif;
  font-weight: 500;
  font-size: 88px;
  text-transform: capitalize;
}
.offer a {
  align-items: center;
}
.offer span {
  margin-top: 0;
  margin-left: 17px;
  font-size: 30px;
}
.offer-slider {
  height: 310px;
}
.swiper-scrollbar-drag {
}
/* .offer-slider .swiper-scrollbar {
  background: rgba(255, 255, 255, 0.5);
}
.offer-slider .swiper-scrollbar-drag {
  background-color: #fff;
} */
.swiper-pagination-bullet {
  padding: 5px 10px;
  border-radius: 0;
  width: auto;
  height: 30px;
  text-align: center;
  line-height: 30px;
  font-size: 12px;
  color: #000;
  opacity: 1;
  background: transparent;
}
.swiper-pagination-bullet-active {
  color: #fbd784;
  background: transparent;
}
.slider {
  display: flex;
  align-items: center;
}
.pages {
  flex-direction: column;
  display: flex;
  align-items: flex-end;
  row-gap: 20px;
  margin-right: 30px;
  z-index: 10;
}
.pages a:focus-within::before {
  content: "";
  width: 3px;
  height: 60px;
  position: absolute;
  right: 0;
  background-color: #fff;
  margin-top: -25px;
}
.slider::after {
  content: "";
  width: 3px;
  height: 240px;
  background-color: #fff;
  opacity: 0.5;
}
/* ============ HERO END ============ */
/* ============ INFO BLOCK START ============ */
.info-section {
  padding-bottom: 100px;
  margin-top: -380px;
}
.info-block {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.info-block:nth-child(odd) {
  padding-left: 150px;
}
.info-block:nth-child(n + 2) {
  margin-top: 200px;
}
.info-block img {
  width: 566px;
  height: 720px;
  object-fit: cover;
}
.info-block:nth-child(odd) > img {
  order: 1;
}
.info-block-content {
  position: relative;
  width: 49%;
}
.info-block-content::before {
  content: attr(data-num);
  position: absolute;
  left: 0;
  top: 0;
  font-size: 240px;
  opacity: 0.1;
  transform: translate(calc(-50% - 28px), calc(-50% + 10px));
}
.info-block-content:nth-child(even) {
  width: 41%;
}
.info-block-content * {
  margin-top: 30px;
}
.info-block-content *:first-child {
  margin-top: 0;
}
.info-block-content h3 {
  font-family: "Chronicle Display", serif;
  font-size: 64px;
  font-weight: 500;
}
.info-block-content p {
  font-weight: 700;
  line-height: 1.77;
}
.info-block-content a {
  color: var(--brand-color);
  align-items: center;
  margin-left: -10px;
}
.info-block-content a:hover {
  color: #fff;
}
.info-block-content span {
  font-size: 29px;
  margin-left: 16px;
}
/* ============ INFO BLOCK END ============ */
/* ============ FOOTER START ============ */
.footer {
  padding-top: 100px;
  padding-bottom: 120px;
  line-height: 1.77;
}
.footer a {
  padding: 10px;
  transition: 0.3s;
  margin-left: -10px;
}
.footer a:hover {
  color: var(--brand-color);
}
.footer-offer {
  width: 300px;
}
.footer .logo {
  margin-top: -10px;
  margin-bottom: 25px;
}
.footer h2 {
  color: var(--brand-color);
  font-size: 24px;
  margin-bottom: 20px;
}
.footer-wrap {
  display: flex;
  justify-content: space-between;
  position: relative;
}
.info-links-wrap {
  display: flex;
  column-gap: 210px;
}
.info-links-list ul {
  font-weight: 500;
}
.info-links-list li:nth-of-type(n + 2) {
  margin-top: 5px;
}
.copyright {
  position: absolute;
  left: 0;
  bottom: 0;
  font-weight: 500;
  opacity: 0.5;
}
.copyright a {
  padding: 0;
  margin: 0 5px;
}
/* ============ FOOTER END ============ */
